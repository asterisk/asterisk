  enableval=$enable_dev_mode; case "${enableval}" in
	      y|ye|yes) AST_DEVMODE=yes ;;
	      n|no)  AST_DEVMODE=no ;;
	      noisy)
	      AST_DEVMODE=yes
	      NOISY_BUILD=yes
	      ;;
	      *) { { $as_echo "$as_me:$LINENO: error: bad value ${enableval} for --enable-dev-mode" >&5
$as_echo "$as_me: error: bad value ${enableval} for --enable-dev-mode" >&2;}
   { (exit 1); exit 1; }; }  ;;
	esac
fi




