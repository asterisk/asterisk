  enableval=$enable_rpath; case "${enableval}" in
		y|ye|yes) check_rpath=yes ;;
		n|no) check_rpath=no ;;
                *) as_fn_error $? "bad value ${enableval} for --disable-rpath" "$LINENO" 5 ;;
	esac
else
  check_rpath=yes
fi


